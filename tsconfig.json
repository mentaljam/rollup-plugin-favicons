{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "sourceMap": false,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "newLine": "lf",
    "esModuleInterop": true,
    "outDir": "dist"
  },
  "include": [
    "src"
  ]
}
